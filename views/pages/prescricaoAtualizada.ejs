<%- contentFor('body') %>
  <%for (listaPrescricoes of listaPrescricoes){%>
    <button class="accordion">
      <%= listaPrescricoes.name%>
        <i class="fas fa-angle-down"></i>
    </button>
    <div class="panel">
      <table style="width:100%">
        <th>Medicamento</th>
        <th>Via</th>
        <th>Intervalo</th>
        <th>Forma Farmacêutica</th>
        <th>Horário</th>
        <th>Lote</th>
        <th>Validade</th>
        <th>Dispensação</th>
        <body>
          <tr>
            <td class="medicamento">
              <%=listaPrescricoes.farm[0].medicamentos%>
            </td>
            <td>
              <%=listaPrescricoes.farm[0].via%>
            </td>
            <td>
              <%=listaPrescricoes.farm[0].intervalo%>
            </td>
            <td>
              <%=listaPrescricoes.farm[0].formaFarmaceutica%>
            </td>
            <td>
              <%=listaPrescricoes.farm[0].horario%>
            </td>
            <td>
              <%=listaPrescricoes.farm[0].lote%>
            </td>
            <td>
              <%=listaPrescricoes.farm[0].validade%>
            </td>
            <td>
              <div class="input">
                <input type="checkbox"> </div>
            </td>
          </tr>
        </body>
      </table>
    </div>
    <%}%>
    