<label for="dieta">Dieta</label>
<table class="tabela-prescricao">
    <tr>
        <th>Tipo</th>
        <th>Intervalo</th>
        <th>Observações</th>
        <th>Edição</th>
    </tr>
    <%for (dieta of prescricao.dieta){%>
        <tr>
            <td>
                <%= dieta.tipo %>
            </td>
            <td>
                <%= dieta.intervalo %>
            </td>
            <td>
                <%= dieta.observacoes %>
            </td>
            <td class="td-botao">
                <div>
                    <a href="<%= updateUrl %>/dieta/<%= dieta.id %>/edit" class="fas fa-edit edit btn btn-outline-primary btn-sm"></a>
                    <form action="<%= updateUrl %>/dieta/<%= dieta.id %>/edit/excluir" method="POST">
                        <button type="submit" onclick="window.location.reload()" class="far fa-trash-alt btn btn-outline-danger btn-sm">
                        </button>
                    </form>
                </div>
            </td>

    </tr>
    <%}%>
</table>