<label for="dieta">Dieta</label>
<table class="tabela-prescricao">
    <tr>
        <th>Tipo</th>
        <th>Intervalo</th>
        <th>Observações</th>
    </tr>
    <%for (dieta of prescricao.dieta){%>
        <tr>
            <td>
                <%= dieta.tipo %>
            </td>
            <td>
                <%= dieta.intervalo %>
            </td>
            <td>
                <%= dieta.observacoes %>
            </td>
            <td>
                <div class="botoes">
                    <a href="<%= updateUrl %>/dieta/<%= dieta.id %>/edit" class="tiracor">
                        <i class="fa fa-edit edit"></i>
                        Editar
                    </a>
                </div>
            </td>
        </tr>
        <%}%>
</table>